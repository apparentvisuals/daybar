{
	"useTabs": false,
	"tabWidth": 2,
	"singleQuote": true,
	"semi": true,
	"trailingComma": "all",
	"printWidth": 120,
	"plugins": [
		"prettier-plugin-svelte",
		"prettier-plugin-tailwindcss"
	],
	"tailwindStylesheet": "./src/routes/layout.css",
	"overrides": [
		{
			"files": "*.svelte",
			"options": {
				"parser": "svelte"
			}
		}
	]
}
